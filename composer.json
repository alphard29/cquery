{
  "name": "cacing69/cquery",
  "description": "HTML document parser for PHP but with jquery taste",
  "keywords": [
    "php",
    "jquery",
    "dom",
    "parser",
    "html",
    "xml",
    "scrape",
    "crawl"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Cacing69\\Cquery\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Ibnul Mutaki",
      "email": "ibnuul@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.0|^8.0",
    "symfony/browser-kit": "^5.0|^6.0",
    "symfony/css-selector": "^5.0|^6.0",
    "symfony/http-client": "^5.0|^6.0",
    "tightenco/collect": "^8.0|^9.0",
    "symfony/deprecation-contracts": "^2.0|^3.0",
    "symfony/dom-crawler": "5.0|^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0|^10.0",
    "symfony/var-dumper": "^5.0|^6.0|^7.0"
  }
}
